{
  "name": "draft-canvas-example",
  "version": "0.0.1-alpha",
  "description": "Draft to Canvas Example",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --open --config webpack.dev.js",
    "dev:auto-reload": "cross-env NODE_ENV=dev-auto-reload webpack-dev-server --progress --open --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cp -R ./src/webpage/static ./dist/",
    "dist:server": "http-server ./dist -o"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "Jewish Interactive Inc.",
  "license": "MIT",
  "devDependencies": {
    "@types/draft-js": "0.10.22",
    "@types/gl-matrix": "^2.4.0",
    "@types/jest": "22.2.3",
    "@types/node": "10.0.3",
    "@types/react": "16.3.13",
    "@types/react-dom": "16.0.5",
    "chokidar": "2.0.3",
    "clean-webpack-plugin": "0.1.19",
    "cors": "^2.8.4",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "express": "^4.16.3",
    "fork-ts-checker-webpack-plugin": "0.4.1",
    "glob": "7.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "install": "^0.11.0",
    "jest": "22.4.3",
    "npm": "^6.0.0",
    "npm-run-all": "4.1.2",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "serve-index": "^1.9.1",
    "shelljs": "0.8.1",
    "source-map-loader": "0.2.3",
    "style-loader": "0.21.0",
    "ts-jest": "22.4.4",
    "ts-loader": "4.2.0",
    "ts-node": "6.0.2",
    "tsconfig-paths": "3.3.1",
    "typescript": "2.8.3",
    "uglify-js": "3.3.23",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.2",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2"
  },
  "dependencies": {
    "draft-js": "0.10.5",
    "draftjs-to-html": "0.8.3",
    "html-to-draftjs": "1.4.0",
    "immutable": "4.0.0-rc.9",
    "react": "16.3.2",
    "react-dom": "16.3.2"
  }
}
